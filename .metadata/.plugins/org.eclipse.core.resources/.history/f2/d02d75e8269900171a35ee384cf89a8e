class MyPanel2 extends JPanel{
	MyOval my_oval = new MyOval(10,10);
	public void paint(Graphics g){
		super.paint(g);
		g.fillOval(my_oval.getX(), my_oval.getY(), 10, 10);
	}
}

class MyOval{
	public int getX() {
		return x;
	}
	public void setX(int x) {
		this.x = x;
	}
	public int getY() {
		return y;
	}
	public void setY(int y) {
		this.y = y;
	}
	int x;
	int y;
	public MyOval(int x,int y){
		this.x = x;
		this.y = y;
	}
}
